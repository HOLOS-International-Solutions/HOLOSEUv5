<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="650"
			 xmlns:properties="clr-namespace:H.Core.Properties;assembly=H.Core"
			 xmlns:viewSections="clr-namespace:H.Avalonia"
             x:Class="H.Avalonia.FarmImportFileView">

	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
	<viewSections:FileImportFarmView />
	<Border IsVisible="{Binding ShowGrid}" BorderBrush="#EDEDED" BorderThickness="0,0,0,1" Margin="0,20" />
	<Button  Classes="back-button" Content="{x:Static properties:Resources.ButtonGoBack}"  HorizontalAlignment="Center" Command="{Binding NavigateToPreviousPage}"/>
	</StackPanel>
</UserControl>

<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="H.Avalonia.FileSaveView">

	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Save Holos" HorizontalAlignment="Center" Margin="0,0,0,15" FontWeight="Bold" FontSize="30" Foreground="#696969" />
		<StackPanel HorizontalAlignment="Center">
			<Button Content="Save" Margin="0 15 0 0" Command="{Binding SaveCommand}"/>
		</StackPanel>
	</StackPanel>
</UserControl>
